{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [""]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "ext-json": "*",
        "bentools/where": "^1.2@dev",
        "guzzlehttp/guzzle": "^6.3",
        "klein/klein": "^2.1",
        "simplon/mysql": "*",
        "twig/twig": "^2.0",
        "vlucas/phpdotenv": "^4.1@dev",
        "ext-iconv": "*",
        "ext-mbstring": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
        ,
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "dev"
}
