{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [""]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "twig/twig": "^2.0",
        "klein/klein": "^2.1",
        "ext-json": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
        ,
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "dev"
}